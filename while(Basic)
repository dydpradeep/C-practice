#include <stdio.h>

int main()
{
    int index=1;
    while(index<=5){
        printf("%d\n",index);
        index++;//index=index+1
    }
    return 0;
}

Output:-
1
2
3
4
5

------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

#include <stdio.h>

int main()
{
    int index=6;
    do{
        printf("%d\n",index);
        index++;//index=index+1
    }while(index<=5);
    return 0;
}

Output:-
6
